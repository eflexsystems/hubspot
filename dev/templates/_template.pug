include _loremipsum.pug

block templateType
	.
		<!--
		templateType: page
		isAvailableForNewContent: true
		-->

doctype html
html
	head
		//- Metadata
		meta( charset='UTF-8' )
		title {{ content.html_title }}
		meta( name='description' content='{{ content.meta_desctiption }}' )
		meta( name="author" content="eFlex Systems" )

		//- Hubspot Header Includes
		| {{ standard_header_includes }}

		//- Fonts
		link( href="https://fonts.googleapis.com/css?family=Titillium+Web:200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&amp;display=swap" rel="stylesheet" )
		link( href="https://fonts.googleapis.com/css2?family=Hind:wght@300;400;500;600;700&display=swap" rel="stylesheet" )

		//- Global Styles
		link( href!="{{ get_public_template_url('/_eflex/styles/stylesheet.css') }}" rel='stylesheet' )

		//- Template Specific Styles
		block styles
			| {{ require_css(get_asset_url("./style.css")) }}

		//- Jquery
		script( src='https://code.jquery.com/jquery-3.3.1.min.js' )

		//- Parallax
		script( src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js" )
		script( src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js" )
		script( src="https://cdn.jsdelivr.net/parallax.js/1.4.2/parallax.min.js" )

		//- Bootstrap
		script( src='https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js' integrity='sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM' crossorigin='anonymous' )

		//- Fontawesome
		script( src="https://kit.fontawesome.com/41afa11259.js" crossorigin="anonymous" )
		link( rel="stylesheet" href="https://use.fontawesome.com/releases/v5.9.0/css/all.css" )

		//- Polyfill
		script( src="https://polyfill.io/v3/polyfill.min.js?features=default" )

	body

		//- Header
		| {% module "module_16031320329341" path="/_eflex/modules/header", label="header", extra_classes="bg-gray-800 d-none d-lg-block" %}

		//- Header Mobile
		| {% module "module_36664828293" path="/_eflex/modules/header_mobile", label="header_mobile", extra_classes="bg-gray-800 d-block d-lg-none" %}

		//- Variables
		block variables

		//- Padding to push content below the height of the header.
		.pt-6
			//- Content
			block content

		//- Hubspot Footer Includes
		| {{ standard_footer_includes }}

		//- Footer
		| {% module "module_16038952826172" path="/_eflex/modules/footer", label="footer" %}

		//- Loading Overlay
		#loadingOverlay.fixed-top.h-100.w-100( style='top: 0; z-index: 1031' )
			.bg-white.position-fixed.h-100.w-100
			.vw-100.vh-100.d-flex.justify-content-center.align-items-center.position-absolute( style='top: 0')
				i.fas.fa-circle-notch.fa-3x.fa-spin.text-gray-200

		script(type='text/javascript')
			//- Use the js added to hubspot later
			include ../script/script.js
			
		//- Template Specific Script
		block script