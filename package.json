{
  "name": "eflex",
  "version": "1.0.0",
  "description": "eflexsystems.com",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean-modules": "rm -r dist/modules && rm -r dist/modules",
    "clean-modules-dev": "rm -r dist/modules && hs fetch --portal=DEV /_eflex/modules dist/modules",
    "copy-js": "cd dev && find . -name '*.js' | cpio -pdmu ../dist && cd ..",
    "watch": "npm run copy-js && ./watch.sh",
    "sync": "npx hscms watch --portal=PROD dist /_eflex",
    "sync-dev": "hs watch dist _eflex --initial-upload",
    "prod": "npm run clean-modules && concurrently --kill-others \"npm run watch\" \"npm run sync\"",
    "dev": "npm run clean-modules-dev && concurrently \"npm run watch\" \"npm run sync-dev\""
  },
  "repository": {
    "type": "git",
    "url": "eflex site"
  },
  "keywords": [
    "eflex",
    "site"
  ],
  "author": "Ryan Jewell",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "@hubspot/cms-cli": "^0.0.22",
    "bootstrap": "^4.5.3",
    "concurrently": "^5.3.0",
    "jquery-parallax.js": "^1.5.0",
    "pug-cli2": "^1.0.0-alpha6.1",
    "slick-carousel": "^1.8.1"
  }
}
